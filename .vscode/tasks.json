{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build (msys2 g++)",
      "type": "shell",
      "command": "C:/msys64/ucrt64/bin/g++.exe",
      "args": [
        "-std=c++17",
        "-O2",
        "-Wall", "-Wextra",
        "main.cpp", "lexer.cpp", "parser.cpp",
        "-o", "prog.exe"
      ],
      "options": {
        "cwd": "${workspaceFolder}",
        "env": {
          "PATH": "C:/msys64/ucrt64/bin;C:/msys64/usr/bin;${env:PATH}"
        }
      },
      "group": "build",
      "problemMatcher": ["$gcc"]
    }
  ]
}
